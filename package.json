{
  "name": "react-landing-ui",
  "version": "1.2.0",
  "description": "A collection of all react components that might be needed to create landing pages",
  "author": "architgarg",
  "license": "MIT",
  "repository": "architgarg/react-landing-ui.git",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "sass"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/react-landing-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/react-landing-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-landing-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/classnames": "^2.3.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "cssnano": "^5.0.14",
    "husky": "^7.0.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.45.1",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react-keyed-flatten-children": "^1.3.0"
  }
}
